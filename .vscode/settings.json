{
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.tabSize": 2,

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },

  "eslint.validate": [
    "typescript",
    "javascript"
  ],
  
  "cSpell.words": [
    "apihost",
    "assethost",
    "chalexpired",
    "imghost",
    "recaptchacompat",
    "reportapi",
    "unticks"
  ]
}
